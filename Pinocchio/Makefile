# Makefile for Pinocchio

all:
	cd asf && $(MAKE)
	cd Pinocchio && $(MAKE)
	cd DemoUI && $(MAKE)

debug:
	cd asf && $(MAKE) debug
	cd Pinocchio && $(MAKE) debug
	cd DemoUI && $(MAKE) debug

depend:
	cd asf && $(MAKE) depend
	cd Pinocchio && $(MAKE) depend
	cd DemoUI && $(MAKE) depend

clean:
	cd asf && $(MAKE) clean
	cd Pinocchio && $(MAKE) clean
	cd DemoUI && $(MAKE) clean


# DO NOT DELETE
